[push]
	default = current
[core]
	editor = vim -c \"set fenc=utf-8\"
[alias]
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  st = status
  chm = checkout master
	ch = checkout
	cm = commit
	ps = push
	pl = pull
	ad = add
	br = branch
	bra = branch -a
	brd = branch -D
